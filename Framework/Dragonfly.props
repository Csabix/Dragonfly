    <BuildMacro Include="DragonflyPackPath">
      <Value>$(DragonflyPackPath)</Value>
    </BuildMacro>
    <BuildMacro Include="RenderdocPath">
      <Value>$(RenderdocPath)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>